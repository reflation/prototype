{
  "compilerOptions": {
    "sourceMap": true,
    "noEmit": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "lib": ["es2015", "es2018", "dom", "dom.iterable"],
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./"
  }
}
